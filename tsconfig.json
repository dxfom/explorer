{
  "compilerOptions": {
    "noEmit": true,
    "target": "es2022",
    "module": "es2022",
    "moduleResolution": "bundler",
    "isolatedModules": true,
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "strict": true,
    "noFallthroughCasesInSwitch": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "skipLibCheck": true,
    "baseUrl": "."
  },
  "include": ["src/**/*"]
}
